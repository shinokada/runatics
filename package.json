{
	"name": "runatics",
	"version": "0.1.4",
	"main": "dist/index.js",
	"description": "A simple Google analyics component for Svelte Runes project",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/runatics/issues",
	"homepage": "https://runatics.codewithshin.com/",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"google analytics",
		"analytics",
		"runes"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/runatics"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"gen:exports": "svelte-lib-helpers exports",
		"gen:docs5": "svelte-lib-helpers docs5FromType",
		"gen:compo-data": "svelte-lib-helpers compo-data",
		"copy:package": "svelte-lib-helpers package",
		"lib-helpers": "pnpm format && pnpm gen:docs5 && pnpm prepack && pnpm copy:package",
		"ch": "npx changeset",
		"cv": "npx changeset version",
		"cp": "npx changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.5",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@playwright/test": "^1.54.2",
		"@sveltejs/adapter-auto": "^6.0.2",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/package": "^2.4.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.1",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/svelte": "^5.2.8",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"flowbite-svelte": "^1.11.5",
		"globals": "^16.3.0",
		"highlight.js": "^11.11.1",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.12",
		"runes-meta-tags": "^0.4.4",
		"runes-webkit": "^0.10.3",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"svelte-lib-helpers": "^0.4.30",
		"svelte-rune-highlight": "^0.7.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite": "^6.3.5",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
